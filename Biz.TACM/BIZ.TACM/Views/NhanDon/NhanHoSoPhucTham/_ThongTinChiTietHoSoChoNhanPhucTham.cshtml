@model Biz.TACM.Models.ViewModel.NhanDon.ChuyenDonViewModel

<div class="modal-header">
    <h5 class="modal-title text-uppercase">Chi Tiết Hồ Sơ</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-block">
                    <table class="table table-information table-hover">
                        <tbody>
                            <tr>
                                <td>Ngày chuyển</td>
                                <td class="text-value-bold">@Model.NgayChuyenDon</td>
                            </tr>
                            <tr>
                                <td>Tòa án chuyển</td>
                                <td class="text-value-bold">@Model.ToaAnChuyenDi</td>
                            </tr>
                            <tr>
                                <td>Người chuyển</td>
                                <td class="text-value-bold">@Model.NguoiTao</td>
                            </tr>
                            <tr>
                                <td>Lý do chuyển</td>
                                <td class="text-value-bold">@Model.LyDoChuyenDon</td>
                            </tr>
                        </tbody>
                    </table>
                    <p class="pl-2-5">Ghi chú</p>
                    <div class="card">
                        <div class="card-block" style="height: 200px; overflow: scroll">
                            @Html.Raw(Model.GhiChu)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>