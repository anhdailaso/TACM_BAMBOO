@using Biz.Lib.TACM.Resources.Resources


<div class="col-12">
    <ul class="list-group">
        <li class="list-group-item d-flex justify-content-between"><span>@ViewText.LABEL_NGAYNHAN</span><span class="font-weight-bold">@string.Format("{0:dd/MM/yyyy}", @Model.NgayNhanHoSo)</span></li>
        <li class="list-group-item d-flex justify-content-between"><span>@ViewText.LABEL_NGUOINHAN</span><span class="font-weight-bold">@Model.NguoiNhanHoSo</span></li>
        <li class="list-group-item d-flex justify-content-between"><span>@ViewText.LABEL_NGAYCAPNHAT</span><span class="font-weight-bold">@string.Format("{0:hh:mm:ss dd/MM/yyyy}", @Model.NgayTao)</span></li>
        <li class="list-group-item d-flex justify-content-between"><span>@ViewText.LABEL_NGUOICAPNHAT</span><span class="font-weight-bold">@Model.NguoiTao</span></li>
    </ul>
        
    <div class="mt-3"><label>@ViewText.LABEL_GHICHU_NHAN</label></div>
    <div class="card">
        <div class="card-block" style="height: 130px; overflow: scroll">
            @Html.Raw(Model.GhiChu)
        </div>
    </div>
</div>

